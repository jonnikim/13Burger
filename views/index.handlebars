<img
  src="https://lh3.googleusercontent.com/proxy/dEVzwqXLp8e0R41cYb1z0mkLZBYON3wF3XBndaeqKTNqdx6ITDETVzcEWWoAzZLphRQdH-fTzqPIEdxIh03-PK-0gA"
  width="250" height="150" class="center">


<div class="main">


  <div class="not-devoured">
    <h2>Burgers not Devoured!</h2>
    <ul>
      {{#each burgers}}
      {{#unless isDevoured}}
      {{> burgers/burger-block isDevoured=true}}
      {{/unless}}
      {{/each}}
    </ul>
  </div>
  <div class="devoured">
    <h2>Burgers Devoured!</h2>

    <ul>
      {{#each burgers}}
      {{#if isDevoured}}
      {{> burgers/burger-block isDevoured=false}}
      {{/if}}
      {{/each}}
    </ul>
  </div>
</div>

<div class="new-burger">
  <h2>Add a Burger</h2>
  <form class="create-form">

    <div class="form-group">
      <label for="ca">Burger:</label>
      <input type="text" id="ca" name="name">
    </div>


    <button type="submit">Add Burger</button>
  </form>
</div>